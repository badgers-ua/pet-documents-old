{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "name": "Debug Jill",
      "port": 9222,
      "urlFilter": "http://localhost:3000/*",
      "webRoot": "${workspaceFolder}/apps/jill"
    },
    {
      "type": "node-terminal",
      "name": "Debug trixie",
      "request": "launch",
      "command": "yarn run start",
      "cwd": "${workspaceFolder}/apps/trixie"
    }
  ]
}
